#include<iostream>
using namespace std;
int main()
{
	char str[1000],c;
	int count=0,i=0;
	puts("Enter the string : ");
	gets(str);
	cout<<"enter the charcter: ";
	cin>>c;
	while(str[i]!='\0')
	{
		if(str[i]==c)
		{
			count++;
		}
		i++;
	 } 
	 cout<<"the Frequency of " <<c<<":: " <<count;
 	return 0;
}
